section.cover
  .container-fluid
    img(class="cover__photo" src="{{ currentUser.picture }}", alt="", height="128", width="128")
    h1.cover__name {{ currentUser.username }}

    | {{ nice }}

    .cover-points
      h2 {{ currentUser.points }} pts

.container-fluid
  .row
    .col-md-7
      form(class="profile-information")
        h2 Basic Information

        fieldset.form-group
          img(class="profile-information__image" src="{{ currentUser.picture }}", alt="", height="128", width="128")

        fieldset.form-group
          label Name
          input(type="text" class="form-control" ng-model="profile.name" readonly)

        fieldset.form-group
          label Username
          input(type="email" class="form-control" ng-model="profile.username" readonly)

        .pull-right
          button(class="btn btn-primary" ng-click="profile.resetAccount()")
            | Reset my account
    
    .col-md-5
      .profile-information.profile-rewards
